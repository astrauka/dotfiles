alias ggm="git checkout master; git pull"
alias gdm="git diff master"
alias ggbr="git for-each-ref --sort=committerdate refs/heads/"
alias ggbrpurge='git branch --merged | grep -v "\*" | grep -v "master" | grep -v "develop" | grep -v "staging" | xargs -n 1 git branch -d'
alias ggpf="git aa; git ci --amend; git push -f"
