// Place your settings in this file to overwrite the default settings
{
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "editor.rulers": [
        120
    ],
    "search.exclude": {
        "**/.bundle": true,
        "**/.sass-cache": true,
        "**/.tmp": true,
        "**/dist": true,
        "**/target": true,
        "**/maven": true,
        "**/coverage": true,
        "**/vendor": true,
        "**/.tscache": true,
        "**/node_modules": true,
        "**/bower_components": true
    },
    "terminal.integrated.shell.osx": "/bin/zsh",
    "eslint.run": "onSave",
    "files.insertFinalNewline": true,
    "workbench.iconTheme": "vscode-icons",
    "files.associations": {
        "*.vm": "velocity",
        "*.mjml": "mjml"
    },
    "explorer.openEditors.visible": 0,
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "files.trimTrailingWhitespace": true,
    "vsicons.dontShowNewVersionMessage": true,
    "window.zoomLevel": 0,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.openPositioning": "left",
    "workbench.editor.revealIfOpen": true,
    "typescript.check.npmIsInstalled": false,
    "eslint.autoFixOnSave": true,
    "editor.minimap.enabled": false,
    "prettier.eslintIntegration": true,
    "editor.formatOnSave": false,
    "workbench.colorTheme": "Quiet Light",
    "workbench.commandPalette.preserveInput": true,
    "editor.autoIndent": true,
    "sync.gist": "28667b4bc715084c71d3904f7e1225f6",
    "sync.lastUpload": "2018-03-05T08:43:34.593Z",
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.lastDownload": "",
    "sync.forceDownload": false,
    "sync.anonymousGist": false,
    "sync.host": "",
    "sync.pathPrefix": "",
    "sync.quietSync": false,
    "sync.askGistName": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.title": "${activeEditorMedium}${separator}${folderName}",
    "editor.accessibilitySupport": "off",
    "workbench.fontAliasing": "antialiased",
    "js-import.codeCompletionAction": false,
    "js-import.codeCompletion": false,
    "js-import.filesToScan": "src/**/*.js",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "editor.renderControlCharacters": false,
    "js-import.maxLen": 120,
    "json.schemas": [
        {
            "fileMatch": [
                "cypress.json"
            ],
            "url": "https://on.cypress.io/cypress.schema.json"
        }
    ],
    "auto-run-command.rules": [],
    "extensions.ignoreRecommendations": false,
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
}
